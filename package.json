{
  "name": "ng-care-providers",
  "version": "1.0.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "build": "rimraf dist && ng build --environment=prod && tsc --project server/tsconfig.app.json",
    "start": "pm2 start dist/server.js --no-daemon",
    "build-start": "npm run build && npm start",
    "dev": "set NODE_ENV=dev && concurrently \"ng serve --progress false --proxy-config proxy.conf.json\" \"npm run api\" ",
    "prod": "set NODE_ENV=prod && npm run build && cd dist && nodemon server.js",
    "api": "cd server && nodemon --ext ts --ignore *.spec.ts --exec ts-node server",
    "test": "concurrently \"ng test --progress false\" \"npm run test-server\" ",
    "test-ng": "ng test",
    "test-server": "cd server && nodemon --ext ts --exec jasmine-ts **/*[!.api].spec.ts",
    "test-api": "cd server && nodemon --ext ts --exec jasmine-ts **/*.api.spec.ts",
    "lint": "ng lint",
    "debug-node": "set NODE_ENV=prod && npm run build && cd dist && iron-node server.js",
    "start-client": "ng serve --progress false --proxy-config proxy.conf.json",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "@ngx-translate/core": "^6.0.1",
    "@ngx-translate/http-loader": "0.0.3",
    "@types/lodash": "^4.14.64",
    "body-parser": "^1.15.2",
    "bootstrap": "^3.3.7",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.4.1",
    "debug": "^2.3.3",
    "express": "^4.15.3",
    "font-awesome": "^4.7.0",
    "lodash": "^4.17.4",
    "mongojs": "^2.4.0",
    "mongoose": "^4.7.2",
    "rxjs": "^5.1.0",
    "typescript-rest": "^1.0.2",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "1.0.3",
    "@angular/compiler-cli": "^4.0.0",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "codelyzer": "~2.0.0",
    "concurrently": "^3.4.0",
    "iron-node": "^3.0.18",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "jasmine-ts": "^0.1.2",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nodemon": "^1.11.0",
    "pm2": "^2.4.6",
    "protractor": "~5.1.0",
    "rimraf": "^2.6.1",
    "ts-node": "^2.0.0",
    "tslint": "~4.5.0",
    "typescript": "^2.2.2"
  }
}
